{
  "name": "face-stamp-app-prototype",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && cp -r dist docs",
    "rebuild": "tsc && vite build && rm -R docs && cp -r dist docs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "fabric": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/fabric": "^5.3.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vladmandic/face-api": "^1.7.12",
    "@yamada-ui/carousel": "^1.0.0",
    "@yamada-ui/dropzone": "^1.0.0",
    "@yamada-ui/react": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.17.4"
  }
